<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="420.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fx.EditWindow">
   <children>
      <VBox prefHeight="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Pane prefHeight="40.0" prefWidth="200.0">
               <children>
                  <HBox alignment="CENTER" prefWidth="420.0" spacing="5.0">
                     <children>
                        <Label fx:id="lbTitle" alignment="CENTER" text="Title">
                           <HBox.margin>
                              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                           </HBox.margin>
                        </Label>
                     </children>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </HBox>
               </children>
            </Pane>
            <ScrollPane>
               <content>
                  <VBox fx:id="editList" prefHeight="520.0" prefWidth="400.0" spacing="5.0" />
               </content>
            </ScrollPane>
            <Pane prefHeight="40.0" prefWidth="200.0">
               <children>
                  <HBox alignment="CENTER_RIGHT" prefWidth="420.0" spacing="5.0">
                     <children>
                        <Button fx:id="btnCancel" alignment="CENTER" mnemonicParsing="false" onAction="#onBtnCancelClick" text="Cancel" />
                        <Button fx:id="btnSave" alignment="CENTER" mnemonicParsing="false" onAction="#onBtnSaveClick" text="Save" />
                     </children>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                  </HBox>
               </children>
            </Pane>
         </children>
      </VBox>
   </children>
</AnchorPane>
