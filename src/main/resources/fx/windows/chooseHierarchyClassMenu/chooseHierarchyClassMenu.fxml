<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>


<AnchorPane maxWidth="260.0" minWidth="260.0" prefHeight="250.0" prefWidth="260.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fx.windows.chooseHierarchyClassMenu.ChooseHierarchyClassMenuController">
   <children>
      <BorderPane>
         <center>
            <VBox>
               <children>
                  <ScrollPane>
                     <content>
                        <ListView fx:id="lwClasses" prefHeight="200.0" />
                     </content>
                  </ScrollPane>
                  <Pane prefHeight="50.0">
                     <children>
                        <HBox alignment="CENTER_RIGHT" prefHeight="50.0" prefWidth="250.0" spacing="5.0">
                           <children>
                              <Region HBox.hgrow="ALWAYS" />
                              <Button fx:id="btnCancel" mnemonicParsing="false" onAction="#onBtnCancelClick" text="Cancel" />
                              <Button fx:id="btnCreate" mnemonicParsing="false" onAction="#onBtnCreateClick" text="Create" HBox.hgrow="NEVER" />
                           </children>
                        </HBox>
                     </children>
                  </Pane>
               </children>
            </VBox>
         </center>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </BorderPane>
   </children>
</AnchorPane>
